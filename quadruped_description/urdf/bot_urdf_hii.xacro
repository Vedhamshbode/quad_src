<?xml version="1.0" ?>
<robot name="FusionComponent" xmlns:xacro="http://www.ros.org/wiki/xacro">

<xacro:arg name="is_sim" default="true"/>

<xacro:include filename="$(find quadruped_description)/urdf/materials.xacro" />
<!-- <xacro:include filename="$(find quadruped_description)/urdf/bot_urdf_hii.trans" /> -->
<xacro:include filename="$(find quadruped_description)/urdf/bot_urdf_hii.gazebo" />
<xacro:include filename="$(find quadruped_description)/urdf/ros2_control.xacro" />
<xacro:include filename="$(find quadruped_description)/urdf/lidar.xacro" />

<xacro:macro name="box_inertia" params="m l w h xyz rpy">
  <inertial>
    <origin xyz="${xyz}" rpy="${rpy}" />
    <mass value="${m}" />
    <inertia ixx="${(m/12)*(h*h + l*l)}" ixy="0" ixz="0"
              iyy="${(m/12)*(w*w +l*l)}" iyz="0"
              izz="${(m/12)*(w*w + h*h)}" />
  </inertial>
</xacro:macro>

