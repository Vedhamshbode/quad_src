<link name="leg_rb_tip">
<inertial>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <mass value="0.01"/> <!-- 10 grams -->
    <!-- Inertia of solid sphere: I = (2/5) * m * r^2 -->
    <inertia 
      ixx="5.12e-08" iyy="5.12e-08" izz="5.12e-08"
      ixy="0.0" iyz="0.0" ixz="0.0"/>
  </inertial>

<visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <sphere radius="0.016"/>
            </geometry>
            <material name="orange"/>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/> 
            <geometry>
                <sphere radius="0.016"/>
            </geometry>
            <surface>
                <friction>
                    <ode>
                        <mu>0.6</mu>
                        <mu2>0.6</mu2>
                        <fdir1>1 0 0</fdir1>
                    </ode>
                </friction>
            </surface>
        </collision>
</link>
